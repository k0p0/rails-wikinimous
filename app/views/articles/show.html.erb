<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h3 class="text-center">Article detail</h3>


      <h4>
       <%= @title %>
      </h4>
      <p><%= @content %></p>

      <%= link_to "Edit article", edit_article_path, class: "btn btn-success" %>
      <%= link_to "Delete article", article_path(@article), method: :delete, class: "btn btn-warning" %>
    </div>
  </div>
</div
